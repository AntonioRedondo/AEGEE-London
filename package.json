		"autoprefixer": "^9.7.6",
		"concurrently": "^5.1.0",
		"connect": "^3.7.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.5.2",
		"cssnano": "^4.1.10",
		"eslint": "^6.8.0",
		"gulp": "^4.0.2",
		"gulp-atomizer": "^3.0.7",
		"gulp-concat": "^2.6.1",
		"gulp-file-include": "^2.1.1",
		"gulp-htmlmin": "^5.0.1",
		"gulp-inline": "^0.1.3",
		"gulp-newer": "^1.4.0",

	"main": "",
	"scripts": {
		"clean": "rimraf docs",
		"lint": "eslint --fix \"**/*.js\" --ignore-pattern \"docs\" && htmlhint src/**/*.htm && stylelint --fix \"**/*.scss\" --ip \"z_atoms.scss\"",
		"gulp": "gulp",
		"gulp:dev": "concurrently \"npm start\" \"gulp dev\"",

		"lint-staged": "^10.1.6",
		"mini-css-extract-plugin": "^0.9.0",
		"opn": "^6.0.0",
		"postcss-assets": "^5.0.0",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^3.0.0",
		"precss": "^4.0.0",
		"rimraf": "^3.0.2",
		"serve-static": "^1.14.1",
		"string-replace-loader": "^2.3.0",
		"style-loader": "^1.1.4",
		"stylelint": "^13.3.2",

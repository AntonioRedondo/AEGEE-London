		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	},
	"browserslist": [
		"safari 9",
		"ie 11"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "eslint --fix",
		"*.htm": "htmlhint",
		"*.scss": "stylelint --fix"
	}
}
